<p class="lead">
    user
</p>
<div id="CourseContainer" class="panel panel-default">
    <div class="panel-heading" id="CourseName"></div>
    <div class="panel-body">
        <div id="CourseDescription"></div>
        <br />
        <a href="#" id="CourseContent" class="lead"></a>
        <br /><br />
        <div class="progress">
            <div class="progress-bar progress-bar-striped active" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 60%">
                @*<span class="sr-only">45% Complete</span>*@
                6 days remaining
            </div>
        </div>
        <div class="form-group row col-md-12">
            <div id="CourseSessionStartDate" class="col-md-6 text-left"></div>
            <div id="CourseSessionEndDate" class="col-md-6 text-right"></div>
        </div>
        <div class="clearfix"></div>
        <hr />
        <div class="form-group checkbox pull-right">
            <label>
                <input type="checkbox" id="ReadTickBox" />
                I have read and understood the contents of the document
            </label>
        </div>
        <div class="clearfix"></div>
        <button id="ReadTickBoxSubmit" type="button" class="editor-btn btn btn-primary pull-right">Submit &raquo;</button>
        @Html.Hidden("CourseSessionId", "-1")
    </div>
</div>
